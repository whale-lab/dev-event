{
    "manifest_version": 2,
    "name": "Dev Event",
    "version" : "1.0.0",
  
    "description" : "개발자 행사를 알려드립니다! ",
    "default_locale" : "ko",
  
    "web_accessible_resources": [
      "static/css/*.css",
      "images/*.png"
    ],
  
    "content_security_policy": "script-src 'self' https://github.com/whale-lab/dev-event ",
    "sidebar_action" : {
      "default_page": "static/index.html",
      "default_icon" : {
        "16": "images/dev-icon.png"
      },
      "default_title" : "Dev Event",
      "use_navigation_bar" : false
    }
  }